{
    "name": "jnjxp/blurb",
    "description": "Simple static content",
    "homepage": "https://github.com/jnjxp/jnjxp.blurb",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "league/plates": "^3.3",
        "mnapoli/front-yaml": "^1.6",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/yaml": "^4.3",
        "mezzio/mezzio": "^3.2",
        "mezzio/mezzio-template": "^2.0",
        "laminas/laminas-dependency-plugin": "^1.0"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Blurb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Blurb\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "config-provider": [
                "Jnjxp\\Blurb\\ConfigProvider"
            ]
        }
    },
    "bin": [
        "bin/blurb"
    ]
}
