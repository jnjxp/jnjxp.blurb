{
    "name": "jnjxp/blurb",
    "description": "Simple static content",
    "homepage":"https://github.com/jnjxp/jnjxp.blurb",
    "license": "MIT",
    "authors": [
        {
            "name": "jake johns",
            "email": "jake@jakejohns.net"
        }
    ],
    "require": {
        "league/plates": "^3.3",
        "mnapoli/front-yaml": "^1.6",
        "phly/phly-expressive-configfactory": "^1.1",
        "psr/container": "^1.0",
        "psr/http-message": "^1.0",
        "symfony/yaml": "^4.3",
        "zendframework/zend-expressive": "^3.2",
        "zendframework/zend-expressive-template": "^2.0"
    },
    "require-dev":{
        "zendframework/zend-diactoros": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Jnjxp\\Blurb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jnjxp\\Blurb\\":"tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "zf": {
            "config-provider": [
                "Jnjxp\\Blurb\\ConfigProvider"
            ]
        }
    }
}
